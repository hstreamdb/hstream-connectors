{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "source-generator",
  "description": "the configuration specification of source-generator",
  "type": "object",
  "properties": {
    "stream": {
      "title": "stream",
      "description": "result stream",
      "type": "string"
    },
    "type": {
      "title": "type",
      "description": "generator type",
      "type": "string",
      "default": "json",
      "enum": ["sequence", "json"]
    },
    "schema": {
      "title": "schema",
      "type": "string",
      "default": "",
      "description": "schema for generating random data"
    },
    "batchSize": {
      "title": "batchSize",
      "description": "batch size",
      "type": "integer",
      "default": 1
    },
    "period": {
      "title": "period",
      "description": "write period",
      "type": "integer",
      "default": 5
    }
  },
  "required": ["stream", "type", "batchSize", "period"],
  "ui:order": ["stream", "type", "schema", "batchSize", "period"]
}